{
  "name": "Octave Streaming - Stream Your Favorite Tunes",
  "short_name": "Octave",
  "description": "Your ultimate music streaming companion",
  "start_url": "/",
  "scope": "/",
  "display_override": ["window-controls-overlay", "standalone"],
  "display": "standalone",
  "orientation": "portrait",
  "background_color": "#000000",
  "theme_color": "#000000",
  "categories": ["music", "entertainment", "streaming"],
  "lang": "en-US",
  "media_control": "android-media-control",
  "media_session": {
      "actions": ["play", "pause", "previoustrack", "nexttrack", "seekto", "seekbackward", "seekforward"],
      "artwork": true
  },
  "dir": "ltr",
  "prefer_related_applications": false,
  "icons": [
    {
      "src": "images/black_logo.png",
      "sizes": "16x16",
      "type": "image/png"
    },
    {
      "src": "images/black_logo.png",
      "sizes": "32x32",
      "type": "image/png"
    },
    {
      "src": "images/black_logo.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "images/black_logo.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ]
}
